{
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "trailing-spaces.trimOnSave": true,
  "workbench.editor.enablePreview": false,
  "editor.formatOnPaste": true,
  "diffEditor.codeLens": true,
  "editor.formatOnType": true,
  "files.associations": {
    "*.html": "html",
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "html.format.wrapLineLength": 120,
  "vscode-w3cvalidation.validator-token": "Wed, 26 May 2021 05:24:16 GMT",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    ".build/**": true,
    "out*/**": true,
    "i18n/**": true,
    "extensions/**/out/**": true,
    "*.min.js": true
  },
  "explorer.openEditors.visible": 0,
  "files.insertFinalNewline": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.useEditorConfig": false,
  "prettier.singleQuote": true,
  "prettier.arrowParens": "avoid",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "webhint.enableTelemetry": "enabled",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.startupEditor": "newUntitledFile",
  "liveServer.settings.donotVerifyTags": true,
  "explorer.confirmDelete": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.git": false
  },
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.colorDecorators": true,
  "tailwindCSS.includeLanguages": {
    "plaintext": "javascript"
  },
  "css.lint.unknownAtRules": "ignore",
  "css.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "prettier.prettierPath": "%AppData%\\npm\\node_modules\\prettier",
  "sqltools.format": {
    "linesBetweenQueries": 2,
    "language": "pl/sql"
  },
  "prettier.vueIndentScriptAndStyle": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "security.workspace.trust.untrustedFiles": "open",
  "tabnine.experimentalAutoImports": true,
  "tabnine.disable_file_regex": ["[.]css$"],
  "colorize.languages": ["javascript", "jsx"],
  "bracketPairColorizer.activeScopeCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}; opacity: 0.5"
  ],
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.profiles.windows": {
    "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe (migrated)": {
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
      "args": []
    }
  },
  "workbench.editor.untitled.hint": "hidden",
  "editor.formatOnSaveMode": "modifications",
  "workbench.iconTheme": "material-icon-theme",
  "cmake.configureOnOpen": false,
  "prettier.printWidth": 120,
  "bracketPairColorizer.depreciation-notice": false,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "redhat.telemetry.enabled": true,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false
  }
}
